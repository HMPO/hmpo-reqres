{
  "name": "hmpo-reqres",
  "version": "1.0.0",
  "description": "Stub request and response objects for testing express applications and middleware",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run examples",
    "test:lint": "eslint ./lib && eslint ./test",
    "test:unit": "mocha ./test --require ./test/helpers --recursive --require choma",
    "examples": "mocha ./examples --recursive --require choma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HMPO/hmpo-reqres.git"
  },
  "keywords": [
    "express",
    "stub",
    "testing",
    "sinon"
  ],
  "author": "HMPO",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "bugs": {
    "url": "https://github.com/HMPO/hmpo-reqres/issues"
  },
  "homepage": "https://github.com/HMPO/hmpo-reqres",
  "dependencies": {
    "express": "^4.17.1",
    "sinon": "^7.5.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "choma": "^1.0.1",
    "eslint": "^3.19.0",
    "mocha": "^6.2.2",
    "sinon-chai": "^2.14.0"
  }
}
